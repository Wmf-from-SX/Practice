void main() {
	char s[20];
	gets(s);
	int m, i, j;
	scanf("%d", &m);
	for (i = 0; i < m; i++) {
		char t;
		t = s[0];
		for (j = 0; j < strlen(s) - 1; j++) {
			s[j] = s[j + 1]; //字符串向左移一位
			s[strlen(s) - 1] = t; //最后一位字符为平移后的s[0]
		}
	}
	for (i = 0; i < strlen(s) - 1; i++)
		printf("%c", s[i]);
}
